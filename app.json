{
  "expo": {
    "name": " ",
    "slug": "sia-moon-staff",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "https://res.cloudinary.com/doez7m1hy/image/upload/w_1024,h_1024,c_fill,f_png,q_auto/Logo%20-%20Black-bg%20-%20Sia%20Moon",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "https://res.cloudinary.com/doez7m1hy/image/upload/w_600,h_600,c_fill,f_png,q_auto/Logo%20-%20Black-bg%20-%20Sia%20Moon",
      "resizeMode": "contain",
      "backgroundColor": "#000000",
      "hideExponentText": true
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.shauntool2u.siamoonstaff",
      "config": {
        "usesNonExemptEncryption": false
      },
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app needs location access to help staff navigate to properties and share their location for task management.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "This app needs location access to help staff navigate to properties and share their location for task management.",
        "NSUserNotificationsUsageDescription": "This app needs notification access to send you important job assignments and updates.",
        "UIBackgroundModes": [
          "remote-notification"
        ]
      }
    },
    "android": {
      "package": "com.shauntool2u.siamoonstaff",
      "adaptiveIcon": {
        "foregroundImage": "https://res.cloudinary.com/doez7m1hy/image/upload/w_1024,h_1024,c_fill,f_png,q_auto/Logo%20-%20Black-bg%20-%20Sia%20Moon",
        "backgroundColor": "#000000"
      },
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "RECEIVE_BOOT_COMPLETED",
        "VIBRATE",
        "WAKE_LOCK",
        "INTERNET",
        "SYSTEM_ALERT_WINDOW",
        "com.google.android.c2dm.permission.RECEIVE",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "favicon": "https://res.cloudinary.com/doez7m1hy/image/upload/w_32,h_32,c_fill,f_png,q_auto/Logo%20-%20Black-bg%20-%20Sia%20Moon",
      "bundler": "metro",
      "serviceWorkerPath": "firebase-messaging-sw.js",
      "backgroundColor": "#000000",
      "template": "./web/index.html",
      "config": {
        "firebase": {
          "notification": {
            "serviceWorkerPath": "firebase-messaging-sw.js"
          }
        }
      }
    },
    "notification": {
      "icon": "https://res.cloudinary.com/doez7m1hy/image/upload/w_64,h_64,c_fill,f_png,q_auto/Logo%20-%20Black-bg%20-%20Sia%20Moon",
      "color": "#8b5cf6"
    },
    "plugins": [
      "expo-router",
      "expo-dev-client",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUseUsageDescription": "Allow Sia Moon Staff to use your location to help navigate to properties and share your location for task management."
        }
      ],
      "expo-font",
      "expo-web-browser",
      "expo-secure-store",
      [
        "expo-notifications",
        {
          "icon": "https://res.cloudinary.com/doez7m1hy/image/upload/w_64,h_64,c_fill,f_png,q_auto/Logo%20-%20Black-bg%20-%20Sia%20Moon",
          "color": "#8b5cf6",
          "sounds": []
        }
      ]
    ],
    "scheme": "sia-moon-staff",
    "experiments": {
      "typedRoutes": true
    },
    "newArchEnabled": true,
    "extra": {
      "router": {},
      "eas": {
        "projectId": "6272f8f0-68ec-4141-a6a9-ae547b7400b2"
      }
    },
    "owner": "shauntool2u"
  }
}
