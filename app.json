{
  "expo": {
    "name": "Sia Moon Staff",
    "slug": "sia-moon-staff",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "https://res.cloudinary.com/doez7m1hy/image/upload/w_1024,h_1024,c_fill,f_png,q_auto/logo-white-vector-3svg_dmz2pf",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "https://res.cloudinary.com/doez7m1hy/image/upload/w_600,h_600,c_fill,f_png,q_auto/logo-white-vector-3svg_dmz2pf",
      "resizeMode": "contain",
      "backgroundColor": "#0B0F1A"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app needs location access to help staff navigate to properties and share their location for task management.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "This app needs location access to help staff navigate to properties and share their location for task management."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "https://res.cloudinary.com/doez7m1hy/image/upload/w_1024,h_1024,c_fill,f_png,q_auto/logo-white-vector-3svg_dmz2pf",
        "backgroundColor": "#0B0F1A"
      },
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ]
    },
    "web": {
      "favicon": "https://res.cloudinary.com/doez7m1hy/image/upload/w_32,h_32,c_fill,f_png,q_auto/logo-white-vector-3svg_dmz2pf",
      "bundler": "metro",
      "serviceWorkerPath": "firebase-messaging-sw.js",
      "config": {
        "firebase": {
          "notification": {
            "serviceWorkerPath": "firebase-messaging-sw.js"
          }
        }
      }
    },
    "notification": {
      "icon": "https://res.cloudinary.com/doez7m1hy/image/upload/w_64,h_64,c_fill,f_png,q_auto/logo-white-vector-3svg_dmz2pf",
      "color": "#8b5cf6"
    },
    "plugins": [
      "expo-router",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Sia Moon Staff to use your location to help navigate to properties and share your location for task management."
        }
      ],
      "expo-font",
      "expo-web-browser",
      "expo-secure-store"
    ],
    "scheme": "sia-moon-staff",
    "experiments": {
      "typedRoutes": true
    },
    "newArchEnabled": true
  }
}
