# Environment Variables Example for Sia Moon Property Management App
# Copy this file to .env.local and fill in your actual values

# Firebase Configuration
EXPO_PUBLIC_FIREBASE_API_KEY=your_firebase_api_key_here
EXPO_PUBLIC_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
EXPO_PUBLIC_FIREBASE_PROJECT_ID=your_project_id
EXPO_PUBLIC_FIREBASE_STORAGE_BUCKET=your_project.appspot.com
EXPO_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=123456789
EXPO_PUBLIC_FIREBASE_APP_ID=1:123456789:web:abcdef123456

# Google Maps API
EXPO_PUBLIC_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Development Settings
EXPO_PUBLIC_USE_FIREBASE_EMULATOR=false
EXPO_PUBLIC_FIREBASE_EMULATOR_HOST=localhost

# Web App API Integration
EXPO_PUBLIC_API_BASE_URL=https://your-web-app.com/api
EXPO_PUBLIC_WEBSOCKET_URL=wss://your-web-app.com/ws
EXPO_PUBLIC_API_KEY=your_mobile_api_key_here
EXPO_PUBLIC_JWT_SECRET=your_jwt_secret_here

# API Endpoints
EXPO_PUBLIC_AUTH_LOGIN_ENDPOINT=/auth/login
EXPO_PUBLIC_AUTH_REFRESH_ENDPOINT=/auth/refresh
EXPO_PUBLIC_BOOKINGS_ENDPOINT=/bookings
EXPO_PUBLIC_TASKS_ENDPOINT=/tasks
EXPO_PUBLIC_PROPERTIES_ENDPOINT=/properties
EXPO_PUBLIC_STAFF_ENDPOINT=/staff
